{
	"version": "0.3.0",
	"configurations": [
		{
			"name": "Next.js: debug full stack, server inspect option",
			"type": "node-terminal",
			"request": "launch",
			"command": "npm run dev:serverInspect",
			"serverReadyAction": {
				"pattern": "- Local:.+(https?://.+)",
				"uriFormat": "%s",
				"action": "openExternally"
			}
		},
		{
			"name": "Next.js: debug full stack",
			"type": "node-terminal",
			"request": "launch",
			"command": "npm run dev",
			"serverReadyAction": {
				"pattern": "- Local:.+(https?://.+)",
				"uriFormat": "%s",
				"action": "openExternally"
			}
		}
	]
}
